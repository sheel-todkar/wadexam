<!DOCTYPE html>
<html>
<head>
  <title>Song List</title>
  <style>
    table, th, td {
      border: 1px solid black;
      border-collapse: collapse;
    }
    th, td {
      padding: 8px;
    }
  </style>
</head>
<body>
  <h1>Song List</h1>
  <table>
    <tr>
      <th>Song Name</th>
      <th>Film</th>
      <th>Music Director</th>
      <th>Singer</th>
      <th>Actor</th>
      <th>Actress</th>
    </tr>
    <% songs.forEach(song => { %>
      <tr>
        <td><%= song.Songname %></td>
        <td><%= song.Film %></td>
        <td><%= song.Music_director %></td>
        <td><%= song.singer %></td>
        <td><%= song.actor || "-" %></td>
        <td><%= song.actress || "-" %></td>
      </tr>
    <% }) %>
  </table>
</body>
</html>
